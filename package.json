{
  "name": "webpack-react-boiler",
  "version": "1.0.0",
  "description": "Simple boilerplate for React with SASS",
  "main": "./src/index.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server",
    "prod": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "build": "npm run lint && npm run lint:sass && npm run clean && npm run prod",
    "lint": "eslint ./src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:sass": "sass-lint ./src/**/*.scss",
    "clean": "rimraf ./dist",
    "test": "echo \"Error: no test specified...yet!\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kkurola/webpack-react-boiler.git"
  },
  "author": "Kimmo Kurola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kkurola/webpack-react-boiler/issues"
  },
  "homepage": "https://github.com/kkurola/webpack-react-boiler#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
